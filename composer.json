{
    "name": "rs/socialite-doccheck",
    "description": "DocCheck ",
    "keywords": [
        "rs",
        "doccheck"
    ],
    "homepage": "https://github.com/rs/socialite-doccheck",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Param Dhaliwal",
            "email": "param@redsnapper.net",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0",
        "ext-json": "*",
        "socialiteproviders/manager": "~4.0"
    },
    "require-dev": {
        "orchestra/testbench": "^8.0",
        "phpunit/phpunit": "^10.0"
    },
    "autoload": {
        "psr-4": {
            "RedSnapper\\SocialiteProviders\\DocCheck\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "RedSnapper\\SocialiteProviders\\DocCheck\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    }
}
